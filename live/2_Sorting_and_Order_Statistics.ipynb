    "class YoungTableu:\n",
    "\n",
    "    def __init__(self, mat):\n",
    "        self.__mat = np.matrix(mat)\n",
    "\n",
    "    def __getitem__(self, index):\n",
    "        return self.__mat[index]\n",
    "\n",
    "    def __setitem__(self, index, val):\n",
    "        self.__mat[index] = val\n",
    "\n",
    "    def __str__(self):\n",
    "        return str(self.__mat)\n",
    "\n",
    "    __repr__ = __str__\n",
    "\n",
    "    def ExtractMin(self):\n",
    "        m, n = self.__mat.shape\n",
    "        index = (0, 0)\n",
    "        mini = self[index]\n",
    "        self[index] = math.inf\n",
    "        while True:\n",
    "            i, j = index\n",
    "            right = (i,j+1)\n",
    "            down = (i+1,j)\n",
    "            smallest = index\n",
    "            if j + 1 < n and self[right] < self[smallest]:\n",
    "                smallest = right\n",
    "            if i + 1 < m and self[down] < self[smallest]:\n",
    "                smallest = down\n",
    "            if smallest == index:\n",
    "                return mini\n",
    "            self[index], self[smallest] = self[smallest], self[index]\n",
    "            index = smallest\n",
    "\n",
    "    def Insert(self, key):\n",
    "        m, n = self.__mat.shape\n",
    "        index = (m-1, n-1)\n",
    "        self[index] = key\n",
    "        while True:\n",
    "            i, j = index\n",
    "            left = (i,j-1)\n",
    "            up = (i-1,j)\n",
    "            largest = index\n",
    "            if j-1 >= 0 and self[left] > self[largest]:\n",
    "                largest = left\n",
    "            if i-1 >= 0 and self[up] > self[largest]:\n",
    "                largest = up\n",
    "            if largest == index:\n",
    "                return\n",
    "            self[index], self[largest] = self[largest], self[index] \n",
    "            index = largest\n",
    "\n",
    "    def __contains__(self, x):\n",
    "        m, n = self.__mat.shape\n",
    "        i, j = m - 1, 0\n",
